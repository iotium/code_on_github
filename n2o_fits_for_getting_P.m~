% fits I made for getting P from mU and mT

function [rho_tg_l, rho_tg_v, u_tg_l, u_tg_v] = n2o_fits_for_getting_P(P)




P_cr = 7245000;
P = P/P_cr;

D_cr = 452.011456;
u_cr = 2.895719271227007e+05;

% version based on rational polynomials

% rho_v:

coeffs = 1e2*[ ...
      -2.776412099306883;
   1.836047824716648;
  -1.754445003937583;
   3.270647148649861;
   0.005700875346075;
  -5.664814006950635;
  -1.393532298412342;
   7.719441106919086];

rho_tg_v = sum(coeffs(1:5) .* P.^[4 3 2 1 0] )...
    /sum( [1; coeffs(6:8)] .* P.^[3 2 1 0] );

% rho_l:

coeffs = 1e3*[...
      -1.204417241378632;
  -2.612093720995419;
  -3.553626341939525;
   9.273665623189361;
   0.157229048830907;
  -3.425072865546610;
   1.436902347189353;
   3.648491437216036;
   0.053787190210862];


rho_tg_l = sum(coeffs(1:5) .* P.^[4 3 2 1 0] )...
    /sum( [1; coeffs(6:9)] .* P.^[4 3 2 1 0] );

% u_l

coeffs = [...
      -2.576869222394410;
   2.430595313525624;
   0.225517834114427;
  -0.003625759454577;
  -3.932436239527763;
   1.738279338172760;
   1.246950115270511;
   0.025101596015874];

u_tg_l = sum(coeffs(1:4) .* P.^[3 2 1 0] )...
    /sum( [1; coeffs(5:8)] .* P.^[4 3 2 1 0] );

% u_v 

coeffs = [...
       0.513150596211608;
   0.505721749966868;
  -5.449791687667358;
   4.343491231891362;
   0.168996031580644;
  -4.498337506869011;
   3.430101917434508;
   0.147394519516129];

u_tg_v = sum(coeffs(1:5) .* P.^[4 3 2 1 0] )...
    /sum( [1; coeffs(6:9)] .* P.^[3 2 1 0] );




% version based on fourier series

%{

% rho_l
log_coeffs = [...
